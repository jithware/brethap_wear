// Install flutter version management: dart pub global activate fvm
// Set current version: fvm use <version>
// For usage see: https://fvm.app/docs/guides/basic_commands
{
  "dart.flutterSdkPath": ".fvm/versions/3.13.8",
  "search.exclude": {
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "cSpell.languageSettings": [
    {
      "languageId": "dart",
      "includeRegExpList": [
        "strings",
        "comments"
      ]
    }
  ],
  "cSpell.ignorePaths": [
    "pubspec.*"
  ]
}